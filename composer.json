{
    "require": {
        "php-di/php-di": "^6.0",
        "selective/config": "^0.1.1",
        "slim/http": "^1",
        "slim/psr7": "^1",
        "slim/slim": "^4.4",
        "slim/twig-view": "^3.0",
        "phpmailer/phpmailer": "^6.1"        
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
